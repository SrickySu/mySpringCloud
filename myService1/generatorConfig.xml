<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection
			connectionURL="jdbc:mysql://localhost:3306/mysql?useSSL=false"
			driverClass="com.mysql.jdbc.Driver" password=""
			userId="root" />
		<javaModelGenerator
			targetPackage="com.surichard.myService1.entity"
			targetProject="myService1\src\main\java" />
		<sqlMapGenerator targetPackage="mybatis.mapper"
			targetProject="myService1\src\main\resources" />
		<javaClientGenerator
			targetPackage="com.surichard.myService1.mapper"
			targetProject="myService1\src\main\java" type="XMLMAPPER" />
		<table schema="mysql" tableName="user"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<!-- <columnOverride column="???" property="???" /> -->
		</table>
	</context>
</generatorConfiguration>